{
  "name": "Arter",
  "version": "1.0.0",
  "description": "A program for managing art",
  "main": "index.js",
  "scripts": {
    "test": "electron . --no-sandbox",
    "build:win": "electron-packager . Arter --platform=win32 --arch=all --out=build/ --icon=icon",
    "build:linux": "electron-packager . Arter --platform=linux --arch=all --out=build/",
    "build:mac": "electron-packager . Arter --platform=darwin --arch=all --out=build/ --icon=icon",
    "build:all": "electron-packager . Arter --all --out=build/ --icon=icon"
  },
  "repository": {
    "type": "git",
    "url": "htasd"
  },
  "author": "ThatCreeper",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.0",
    "about-window": "^1.13.2",
    "custom-electron-titlebar": "^3.2.0"
  },
  "devDependencies": {
    "electron": "^15.5.5",
    "electron-packager": "^14.1.1"
  }
}
